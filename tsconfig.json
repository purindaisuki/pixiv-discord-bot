{
  "extends": "ts-node/node16/tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*"]
    }
  },
  "include": ["src/**/*", "deploy-commands.ts", "extend-expect.ts"]
}
